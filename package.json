{
  "name": "test11",
  "version": "1.0.0",
  "description": "Miyabi Framework - Autonomous Development System with 7 AI Agents",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "state:transition": "tsx scripts/state-transition.ts",
    "webhook:route": "tsx scripts/webhook-router.ts",
    "agent:run": "tsx scripts/agent-runner.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"scripts/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "miyabi",
    "autonomous",
    "ai-agents",
    "github-automation",
    "shikigaku",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@octokit/rest": "^20.1.1",
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
